<?xml version="1.0" encoding="utf-8" ?>

<GraphSettings>

  <DefaultSettings width="300" height="200"/>

  <Graphs>

    <!-- System counters -->
    <add sender="*" fileName="CPUTotal.png" title="CPU Total Usage %" timeSpan="00:10:00" lowerLimit="0" upperLimit="100">
      <Items>
        <add fileName="System.CpuTotal.rrd" dataSource="GAUGE" cf="AVERAGE" type="Area" color="Blue"/>
      </Items>
    </add>

    <add sender="*" fileName="CPU.png" title="CPU Usage %" timeSpan="00:10:00" lowerLimit="0" upperLimit="100">
